<?xml version="1.0" encoding="utf-8" ?>
<FixOne>
	<LogToTrace>
		<Settings level="Info"/>
	</LogToTrace>
	<LogToTextFile>
		<Settings
			level="Debug"
			path="..\\DebugLogs\\Server"/>
	</LogToTextFile>
	<Engine>
		<EnabledLoggers>
			<item>Log to System Trace</item>
			<item>Log to Text File</item>
		</EnabledLoggers>
		<EnabledStorages>
			<item>XML Persistent Storage</item>
		</EnabledStorages>
	</Engine>
</FixOne>
